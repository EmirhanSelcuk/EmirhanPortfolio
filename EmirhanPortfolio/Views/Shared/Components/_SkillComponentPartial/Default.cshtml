@model List<EmirhanPortfolio.DAL.Entities.Skill>
<div class="col-six tab-full right">
    <h3>Yeteneklerim</h3>

    <ul class="skill-bars">
        @foreach(var item in Model)
        {
            <li>
                <div class="progress percent90"><span>@item.Value%</span></div>
                <strong>@item.SkillTitle</strong>
            </li>
        }

    </ul>
</div>